			"category": [
				{
					"id": "1",
					"name": "Salary",
					"userId": "1"
				},
				{
					"id": "2",
					"name": "Other",
					"userId": "1"
				}
			]
		}

		Description: Creates given source
		Method: POST
		Parameters: /
		Accepts: application/json
		Accepted body example:
		{
			"name": "Drugs and contraband",
			"userId": "1"
		}
		Sends: 
		Sent body example:	

